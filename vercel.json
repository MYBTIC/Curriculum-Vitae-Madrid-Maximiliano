{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "./",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/public/dist/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/cv",
      "destination": "/"
    }
  ]
}