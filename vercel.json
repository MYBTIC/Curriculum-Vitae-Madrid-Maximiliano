{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/output.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "dist/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}