{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/output.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "dist/$1"
    },
    {
      "src": "/MiFoto.jpg",
      "dest": "MiFoto.jpg"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}